{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "a vue template project.",
  "main": "main.ts",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ]
  },
  "scripts": {
    "test": "",
    "prepare": "husky install",
    "precommit": "eslint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "changelog:auto": "auto-changelog",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "check:types": "tsc --watch",
    "build:dev": "webpack --config ./build/webpack.dev.js",
    "build:prod": "webpack --config ./build/webpack.prod.js",
    "dev": "webpack serve --config ./build/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Inchill/vue-template.git"
  },
  "keywords": [
    "vue",
    "template",
    "cli"
  ],
  "author": "Inchill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Inchill/vue-template/issues"
  },
  "homepage": "https://github.com/Inchill/vue-template#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vue/compiler-sfc": "^3.0.11",
    "auto-changelog": "^2.2.1",
    "babel-loader": "^8.2.2",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "css-loader": "^5.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-plugin-vue": "^7.9.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^6.0.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "^16.2.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@babel/runtime-corejs3": "^7.14.0",
    "core-js": "^3.13.1",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1"
  }
}
